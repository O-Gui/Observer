public class Interruptor {
	
	private interruptorListerner listener;
	
	public void addListener(interruptorListerner listener) {
		
		this.listener = listener;	
	}

	public void ligar() {
		
		listener.aoligar();
	}
	
	public void desligar() {
		
		listener.aodesligar();
